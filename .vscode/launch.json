    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "IRL",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/play_irl_gui.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "AUTO",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/play_auto.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        }
    
    ]
}